{
  "manifest_version": 3,
  "name": "newTabInterface",
  "description": "A cool extension which runs in Chrome New Tab. Developed By: https://tarikul.dev",
  "version": "4.0.0",
  "permissions": ["tabs", "storage"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "./newTabIcon16.png",
    "48": "./newTabIcon48.png",
    "128": "./newTabIcon128.png"
  },
  "action": {
    "default_icon": {
      "16": "./newTabIcon16.png",
      "48": "./newTabIcon48.png",
      "128": "./newTabIcon128.png"
    }
  }
}